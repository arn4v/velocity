{
  "name": "velocity",
  "license": "MIT",
  "author": "Arnav Gosain <arnav@arnavgosain.com> (https://arnavgosain.com)",
  "scripts": {
    "next:dev": "next dev",
    "start:ci": "yarn db:migrate:deploy && yarn start",
    "build": "next build",
    "dev": "run-p next:dev db:gen:dev",
    "start": "next start",
    "db": "prisma studio --port 5556 --browser none",
    "db:gen": "prisma generate",
    "db:gen:dev": "prisma generate --watch",
    "db:push": "prisma db push --skip-generate",
    "db:migrate:create": "prisma migrate dev --create-only --skip-generate --skip-seed",
    "db:migrate:dev": "prisma migrate dev --skip-generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.7.0",
    "@prisma/client": "^3.10.0",
    "@trpc/client": "^9.20.1",
    "@trpc/next": "^9.20.1",
    "@trpc/react": "^9.20.1",
    "@trpc/server": "^9.20.1",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "superjson": "^1.8.1",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.4"
  },
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": true,
    "arrowParens": "always"
  }
}

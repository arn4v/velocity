{
  "name": "velocity",
  "license": "MIT",
  "author": "Arnav Gosain <arnav@arnavgosain.com> (https://arnavgosain.com)",
  "scripts": {
    "next:dev": "next dev",
    "start:ci": "yarn db:migrate:deploy && yarn start",
    "build": "next build",
    "dev": "run-p next:dev db:gen:dev",
    "start": "next start",
    "db": "prisma studio --port 5556 --browser none",
    "db:gen": "prisma generate",
    "db:gen:dev": "prisma generate --watch",
    "db:push": "prisma db push --skip-generate",
    "db:migrate:create": "prisma migrate dev --create-only --skip-generate --skip-seed",
    "db:migrate:dev": "prisma migrate dev --skip-generate",
    "db:migrate:deploy": "prisma migrate deploy",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^1.7.0",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "^3.13.0",
    "@trpc/client": "^9.23.2",
    "@trpc/next": "^9.23.2",
    "@trpc/react": "^9.23.2",
    "@trpc/server": "^9.23.2",
    "framer-motion": "^6",
    "next": "latest",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.38.1",
    "superjson": "^1.9.1",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "prisma": "^3.13.0",
    "typescript": "4.6.4"
  },
  "prettier": {
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": true,
    "arrowParens": "always"
  }
}
